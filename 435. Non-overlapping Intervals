class Solution:
    def eraseOverlapIntervals(self, intervals: List[List[int]]) -> int:
        if intervals == []: return 0
        count = 0
        s = sorted(intervals,key= lambda x:x[1])
        i = 1
        n = len(s)
        start = s[0][0]
        end = s[0][1]
        ans = [[start,end]]
        while i < n:
            if s[i][0] >= ans[-1][1]:
                ans.append(s[i])
            i += 1
                
        return n-len(ans)
